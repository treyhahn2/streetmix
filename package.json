    "handjs": "1.3.11",
    "i18next": "3.3.1",
    "i18next-xhr-backend": "0.6.0",
    "jade": "1.11.0",
    "jquery": "3.0.0",
    "js-cookie": "2.1.2",
    "kerberos": "0.0.21",
    "lodash": "4.13.1",
    "moment": "2.13.0",
    "mongoose": "4.5.2",
    "newrelic": "1.28.1",
    "node-env-file": "0.1.8",
    "node-sass": "3.8.0",
    "oauth": "0.9.14",
