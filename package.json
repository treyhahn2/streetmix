    "tap-spec": "4.1.1",
    "tape": "4.6.0",
    "tape-catch": "1.0.6"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "./assets/scripts/vendor/FileSaver.js": "saveAs"
  }
}
