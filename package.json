    "superagent": "2.0.0",
    "supervisor": "0.11.0",
    "twitter": "1.3.0",
    "uuid": "2.0.2",
    "validator": "5.4.0",
    "vinyl-fs": "2.4.3",
    "whatwg-fetch": "1.0.0",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "eslint": "2.13.1",
    "eslint-config-standard": "5.3.5",
    "eslint-config-standard-react": "3.0.0",
    "eslint-plugin-promise": "2.0.1",
